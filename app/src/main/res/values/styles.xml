<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mainbutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:backgroundTint">@color/teal_700</item>
    </style>

    <style name="buttonoutlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="cornerRadius">7dp</item>
        <item name="backgroundTint">@android:color/darker_gray</item>
        <item name="strokeColor">@android:color/holo_blue_light</item>
    </style>

